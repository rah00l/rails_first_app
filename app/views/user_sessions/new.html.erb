 <%= "Log in" %>  
   
 <% form_for @user_session do |form| %>  
 <%= form.error_messages %>  
 <ul class="formList">  
   <li>  
     <%= form.label :username, "Username" %>  
     <%= form.text_field :username %>  
   </li>  </br>
   <li>  
     <%= form.label :password, "Password" %>  
     <%= form.password_field :password %>  
   </li> 
    </ul>  
 
   <div>  
     <%= form.submit "Submit" %>  
   </div>  
 <% end %>